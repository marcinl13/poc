{
  "name": "nextjs-clerk-chat-with-redis",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "chat",
    "server-ws",
    "shared"
  ],
  "scripts": {
    "app:dev": "pnpm --filter=chat dev",
    "app:server-ws:dev": "pnpm --filter=server-ws dev",
    "app:server-ws:lint": "pnpm --filter=server-ws lint",
    "app:server-ws:lint:fix": "pnpm --filter=server-ws lint:fix",
